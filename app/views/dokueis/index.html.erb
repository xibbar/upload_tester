<h1>Listing dokueis</h1>

<table>
  <tr>
    <th>Title</th>
  </tr>

<% @dokueis.each do |dokuei| %>
  <tr>
    <td><%=h dokuei.title %></td>
    <td><%= link_to 'Show', dokuei %></td>
    <td><%= link_to 'Edit', edit_dokuei_path(dokuei) %></td>
    <td><%= link_to 'Destroy', dokuei, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New dokuei', new_dokuei_path %>